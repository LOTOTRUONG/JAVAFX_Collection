<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="150.0" maxWidth="260.0" minHeight="150.0" minWidth="260.0"
      prefHeight="150.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/21"
      fx:controller="main.collection.Controller.PaneAttributModifyController">
    <children>
        <TextField fx:id="modifiedAttributTextField" alignment="CENTER" maxHeight="25.0" maxWidth="240.0"
                   minHeight="25.0" minWidth="240.0" prefHeight="25.0" prefWidth="240.0">
            <VBox.margin>
                <Insets bottom="20.0" left="20.0" right="20.0"/>
            </VBox.margin>
        </TextField>
        <Button maxHeight="25.0" maxWidth="90.0" minHeight="25.0" minWidth="90.0" mnemonicParsing="false"
                onAction="#saveModifiedAttribut" prefHeight="25.0" prefWidth="90.0" text="Save"/>
    </children>
    <padding>
        <Insets bottom="20.0" top="20.0"/>
    </padding>
</VBox>
