<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/21" fx:controller="main.collection.Controller.PaneMainController">
    <center>
        <AnchorPane prefHeight="720.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="loginButton" layoutX="356.0" layoutY="317.0" mnemonicParsing="false"
                        onAction="#enterLogin" prefHeight="41.0" prefWidth="188.0" text="Se Connecter"/>
                <Button fx:id="registerButton" layoutX="655.0" layoutY="317.0" mnemonicParsing="false"
                        onAction="#enterRegister" prefHeight="41.0" prefWidth="224.0" text="Registre"/>
            </children>
        </AnchorPane>
    </center>
    <top>
        <fx:include minWidth="1280.0" prefHeight="30.0" prefWidth="1280.0" source="MenuBar.fxml"/>
    </top>
</BorderPane>
